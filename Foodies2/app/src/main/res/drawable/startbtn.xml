<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <corners
                android:radius="6dp"
                />
            <solid
                android:color="#cc1bcf9c"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="110dp"
                android:height="48dp"
                />
            <stroke
                android:width="3dp"
                android:color="#1F1F1F"
                />
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <corners
                android:radius="6dp"
                />
            <solid
                android:color="#bd15bbc1"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="110dp"
                android:height="48dp"
                />
            <stroke
                android:width="1dp"/>
                />
        </shape>
    </item>
</selector>