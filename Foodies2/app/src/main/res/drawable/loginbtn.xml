<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <stroke
                android:width="1dp"/>
            <solid
                android:color="#F1F6BE" />
            <corners
                android:radius="15dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>

    <item>
        <shape>
            <stroke
                android:width="1dp"/>
            <corners
                android:radius="15dp" />
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="10dp"
                android:bottom="10dp" />
            <solid
                android:color="#D7EB5A"
                />

        </shape>
    </item>
</selector>